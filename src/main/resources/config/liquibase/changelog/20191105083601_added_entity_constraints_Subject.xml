<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Subject.
    -->
    <changeSet id="20191105083601-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="department_id"
                                 baseTableName="subject"
                                 constraintName="fk_subject_department_id"
                                 referencedColumnNames="id"
                                 referencedTableName="department"/>

        <addForeignKeyConstraint baseColumnNames="batch_id"
                                 baseTableName="subject"
                                 constraintName="fk_subject_batch_id"
                                 referencedColumnNames="id"
                                 referencedTableName="batch"/>

    </changeSet>
</databaseChangeLog>
